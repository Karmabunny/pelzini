TABLE files
PK id
  id serial
  name varchar(255) not null
  description text
  source mediumtext not null
  packageid int
  sinceversion varchar(255)
  
  
TABLE classes
PK id
  id serial
  fileid int not null
  name varchar(255) not null
  description text
  abstract smallint not null
  extends varchar(255)
  visibility varchar(20) not null
  final smallint not null
  sinceversion varchar(255)
  
  
TABLE interfaces
PK id
  id serial
  fileid int not null
  name varchar(255) not null
  description text
  extends varchar(255)
  visibility varchar(20) not null
  sinceversion varchar(255)
  
  
TABLE functions
PK id
  id serial
  fileid int not null
  classid int
  interfaceid int
  name varchar(255) not null
  description text
  arguments varchar(255) not null
  visibility varchar(20)
  returntype varchar(255)
  returndescription text
  static smallint not null
  final smallint not null
  sinceversion varchar(255)
  
  
TABLE arguments
PK id
  id serial
  functionid int not null
  name varchar(255) not null
  type varchar(255)
  defaultvalue varchar(255)
  description text
  
  
TABLE variables
PK id
  id serial
  classid int
  interfaceid int
  name varchar(255) not null
  type varchar(255)
  description text
  static smallint not null
  sinceversion varchar(255)
  
  
TABLE constants
PK id
  id serial
  fileid int not null
  name varchar(255) not null
  value varchar(255) not null
  description text
  sinceversion varchar(255)
  
  
  
  
TABLE authors
PK id
  id serial
  linkid int not null
  linktype smallint not null
  name varchar(255) not null
  email varchar(255) not null
  description text
  
  
TABLE packages
PK id
  id serial
  name varchar(50) not null
  
  
TABLE projects
PK id
  id serial
  name varchar(255) not null
  license text
