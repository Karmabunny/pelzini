TABLE Files
ENGINE MyISAM
PK ID
  ID int unsigned NOT NULL auto_increment
  Name varchar(255) NOT NULL
  Description text
  Source mediumtext NOT NULL
  PackageID int unsigned
  SinceVersion varchar(255)
  
  
